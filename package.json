{
  "name": "react-app-base",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "test": "mocha --recursive --compilers js:babel-core/register",
    "test:watch": "npm test -- --watch",
    "build:server": "babel server -d build/server --experimental && cp ./env.json build/ && cp -R ./views build && cp -R  public build/",
    "build:app": "babel app -d build/app --experimental",
    "build": "webpack && npm run build:server && npm run build:app",
    "server:web": "nodemon --exec babel-node server/index",
    "server:webpack": "webpack-dev-server --host localhost --port 8082 --content-base public --history-api-fallback --hot --inline --progress",
    "lint": "esw src/",
    "lint:w atch": "npm run lint -- -w; exit 0",
    "start:dev": "npm run server:web & npm run server:webpack",
    "start": "NODE_ENV=production node build/server/index.js",
    "postinstall": "npm run build"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "app": "0.0.1",
    "babel": "^5.8.21",
    "babel-loader": "^5.3.2",
    "body-parser": "^1.13.3",
    "eslint": "^1.3.1",
    "eslint-watch": "^2.1.0",
    "exphbs": "^1.0.1",
    "express": "^4.13.3",
    "express-handlebars": "^2.0.1",
    "history": "^1.9.0",
    "qajax": "^1.3.0",
    "react": "^0.14.0",
    "react-dom": "^0.14.0",
    "react-redux": "^2.0.0",
    "react-router": "^1.0.0-rc1",
    "redux": "^2.0.0",
    "redux-thunk": "^0.1.0",
    "webpack": "^1.11.0",
    "webpack-dev-server": "^1.10.1"
  },
  "devDependencies": {
    "autoprefixer-loader": "^3.1.0",
    "babel-core": "^5.8.22",
    "babel-eslint": "^4.1.1",
    "css-loader": "^0.21.0",
    "eslint-config-airbnb": "0.0.8",
    "eslint-plugin-react": "^3.3.1",
    "expect": "^1.10.0",
    "mocha": "^2.3.2",
    "node-sass": "^3.4.1",
    "react-hot-loader": "^1.2.8",
    "sass-loader": "^3.1.1",
    "style-loader": "^0.13.0"
  }
}
